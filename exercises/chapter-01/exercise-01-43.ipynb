   "metadata": {},
   "outputs": [],
   "source": [
    "(define (compose f g)\n",
    "  (lambda (x)\n",
    "    (f (g x))))\n",
    "\n",
    "(define (repeated f n)\n",
    "  (if (= n 1)\n",
    "      f\n",
    "      (compose f (repeated f (dec n)))))"
